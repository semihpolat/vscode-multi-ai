{
  "name": "vscode-multi-ai",
  "displayName": "vscode-multi-ai",
  "description": "multi lang/framework ai refactoring",
  "version": "0.0.2",
  "publisher": "SemihP", 
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "main": "./dist/extension.js",
  "activationEvents": [
    "onCommand:vscode-multi-ai.helloWorld",
    "onCommand:vscode-multi-ai.refactorWithChatgpt",
    "onCommand:vscode-multi-ai.codeFromZero",
    "onCommand:vscode-multi-ai.setApiKey"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-multi-ai.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vscode-multi-ai.refactorWithChatgpt",
        "title": "Refactor it!ðŸš€"
      },
      {
        "command": "vscode-multi-ai.codeFromZero",
        "title": "From Zero to Hero!ðŸ¤º"
      },
      {
        "command": "vscode-multi-ai.setApiKey",
        "title": "Set Gemini API Key"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "vscode-multi-ai.refactorWithChatgpt",
          "when": "editorHasSelection",
          "group": "navigation"
        },
        {
          "command": "vscode-multi-ai.codeFromZero",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.3.9",
    "axios": "^0.21.1",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
